    <Content Include="reservation_5.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="room_in_accommodation.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
